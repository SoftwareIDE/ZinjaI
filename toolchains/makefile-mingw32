type=extern
desc=makefile based building (mingw32)
build_command=mingw32-make ${ARG1} ${ARG2}
clean_command=mingw32-make ${ARG1} ${ARG3}
argument1=Common arguments=-f Makefile
argument2=Build arguments=-j ${NUM_PROCS} all
argument3=Clean arguments=clean
use_return_vale=1
bin_path=MinGW/bin
base_path=MinGW